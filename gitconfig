  ui = true
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
[user]
  name = Vincent Le
  email = vinnyoodles@gmail.com
[push]
  default = matching
[core]
  editor = /usr/bin/vim
[alias]
  logp = log --pretty=oneline --abbrev-commit --graph --decorate --color --all
  cmt = commit -m
  cmp = "!f() { git commit -m \"$@\" && git push; }; f"
  chk = checkout
  d = diff
  p = push
  pop = reset --soft HEAD^
  pullup = pull upstream master
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = blue bold
[diff]
  tool = vimdiff
[difftool]
  prompt = false
